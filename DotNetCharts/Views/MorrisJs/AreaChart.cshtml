@{
    ViewBag.Title = "Area Chart";
    Layout = "~/Views/Shared/_morrisjs.cshtml";
}

<div class="row pt-3 px-4">
    <div class="col-md-6">
        <div class="card">
            <div class="card-header">
                Simple Area Chart
            </div>
            <div class="card-body">
                <div id="graph"></div>
            </div>
        </div>
    </div>
</div>

<script>
    Morris.Area({
        element: 'graph',
        data: [
            { y: '2006', a: 100, b: 90 },
            { y: '2007', a: 75, b: 65 },
            { y: '2008', a: 50, b: 40 },
            { y: '2009', a: 75, b: 65 },
            { y: '2010', a: 50, b: 40 },
            { y: '2011', a: 75, b: 65 },
            { y: '2012', a: 100, b: 90 }
        ],
        xkey: 'y',
        ykeys: ['a', 'b'],
        labels: ['Series A', 'Series B']
    });
</script>