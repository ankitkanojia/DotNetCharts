@{
    ViewBag.Title = "Bar Chart";
    Layout = "~/Views/Shared/_morrisjs.cshtml";
}

<div class="row mt-5 mb-5">
    <div class="col-md-6">
        <div class="card">
            <div class="card-header">
                Simple Bar Chart
            </div>
            <div class="card-body">
                <div id="graph"></div>
            </div>
        </div>
    </div>
</div>

<script>
    Morris.Bar({
        element: 'graph',
        data: [
            { x: '2011 Q1', y: 3, z: 2, a: 3 },
            { x: '2011 Q2', y: 2, z: null, a: 1 },
            { x: '2011 Q3', y: 0, z: 2, a: 4 },
            { x: '2011 Q4', y: 2, z: 4, a: 3 }
        ],
        xkey: 'x',
        ykeys: ['y', 'z', 'a'],
        labels: ['Y', 'Z', 'A']
    }).on('click', function (i, row) {
        console.log(i, row);
    });
</script>